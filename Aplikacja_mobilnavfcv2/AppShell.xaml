<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Aplikacja_gierki.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Aplikacja_gierki.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Aplikacja_gierki">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:HomePage}"
        Route="HomePage" />
    <ShellContent
        Title="Main"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Blur"
        ContentTemplate="{DataTemplate local:BlurHomePage}"
        Route="BlurHomePage" />
    <ShellContent
        Title="Second"
        ContentTemplate="{DataTemplate local:SecondPage}"
        Route="SecondPage" />
    <ShellContent
        Title="Summary"
        ContentTemplate="{DataTemplate local:SummaryPage}"
        Route="SummaryPage" />
</Shell>
